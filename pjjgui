local addToTortureListPanel = nil
local removeFromTortureListPanel = nil

guiAPI.newTabObject(goofyTab, baseGUI, "textlabel", {
	["size"] = UDim2.new(1,0,0.1,0),
	["text"] = [[its goofin time]],
	["color"] = Color3.fromRGB(255,255,255),
